{
  "name": "airtable-ts-codegen",
  "version": "1.2.0",
  "description": "Autogenerate TypeScript definitions for your Airtable base",
  "license": "MIT",
  "author": "Adam Jones (domdomegg)",
  "repository": {
    "type": "git",
    "url": "https://github.com/domdomegg/airtable-ts-codegen.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "airtable-ts-codegen": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run build && node dist/cli.js",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "clean": "rm -rf dist",
    "build": "tsc --project tsconfig.build.json",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "dependencies": {
    "@kristiandupont/recase": "^1.2.1",
    "axios": "^1.6.8",
    "diacritics": "^1.3.0"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^20.1.3",
    "@types/diacritics": "^1.3.3",
    "@types/node": "^20.12.8",
    "airtable-ts": "^1.4.0",
    "eslint": "^8.57.0",
    "eslint-config-domdomegg": "^1.2.3",
    "tsconfig-domdomegg": "^1.0.0",
    "typescript": "^5.4.4",
    "vitest": "^3.0.7"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-domdomegg"
    ],
    "rules": {
      "object-curly-newline": [
        "error",
        {
          "multiline": true,
          "consistent": true
        }
      ]
    }
  }
}
